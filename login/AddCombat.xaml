<Window x:Class="login.AddCombat"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:login"
        mc:Ignorable="d"
        Title="AddCombat" Height="300" Width="300">
    <Grid>
        <ComboBox x:Name="boxerLocal" HorizontalAlignment="Left" Margin="15,44,0,0" VerticalAlignment="Top" Width="120" SelectionChanged="boxerLocal_SelectionChanged"/>

        <ComboBox x:Name="boxerRival" HorizontalAlignment="Left" Margin="162,44,0,0" VerticalAlignment="Top" Width="120" SelectionChanged="boxerRival_SelectionChanged"/> 
        <RadioButton x:Name="radioLocal" GroupName="Victory" Content="" HorizontalAlignment="Left" Margin="68,93,0,0" VerticalAlignment="Top" IsChecked="True"/>
        <RadioButton x:Name="radioRival" GroupName="Victory"  Content="" HorizontalAlignment="Left" Margin="215,93,0,0" VerticalAlignment="Top"/>
        <CheckBox x:Name="checkKO" Content="KO" HorizontalAlignment="Left" Margin="112,113,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label" Content="Ganador" HorizontalAlignment="Left" Margin="112,82,0,0" VerticalAlignment="Top" Width="72" />


    </Grid>
</Window>
